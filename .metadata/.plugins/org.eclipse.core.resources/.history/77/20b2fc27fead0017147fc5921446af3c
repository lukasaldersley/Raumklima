public class tst
{
    public void Help() {
        try {
            InputStream is = getClass().getResourceAsStream("PDF_DATEI.pdf");
            byte[] data = new byte[is.available()];
            is.read(data);
            is.close();
            String tempFile = "PDF_DATEI";
            File temp = File.createTempFile(tempFile, ".pdf");
            FileOutputStream fos = new FileOutputStream(temp);
            fos.write(data);
            fos.flush();
            fos.close();
            temp
        } catch (IOException e) {
            e.printStackTrace();
        }
    }
}
